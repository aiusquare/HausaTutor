.toggleWrapper input.mobileToggle + label::after { content: ""; position: absolute; display: block; box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 0px 1px, rgba(0, 0, 0, 0.04) 0px 4px 0px 0px, rgba(0, 0, 0, 0.13) 0px 4px 9px, rgba(0, 0, 0, 0.05) 0px 3px 3px; transition: all 0.35s cubic-bezier(0.54, 1.6, 0.5, 1) 0s; background: rgb(245, 245, 245); height: 20px; width: 20px; top: 1px; left: 2px; border-radius: 60px; }

.toggleWrapper input.mobileToggle:checked + label::before { background: rgb(4, 172, 237); transition: width 0.2s cubic-bezier(0, 0, 0, 0.1) 0s; content: "Yes"; color: rgb(255, 255, 255); text-align: left; padding: 2px 0px 0px 8px; }

.toggleWrapper input.mobileToggle:checked + label::after { left: 37px; }

